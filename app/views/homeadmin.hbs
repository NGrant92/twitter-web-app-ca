{{> mainmenu _id="home" }}

<div class="ui equal width padded grid">
  <div class = "four wide column">
    {{> profilecard}}
  </div>

  <div class = "eight wide align right column">

    <section class="ui segment" id="timelineTabs" style=overflow:auto;max-height:1200px;min-height:200px;">
      <h3 class="ui dividing header">Activty Info</h3>
      <div class="ui pointing secondary demo menu">
        <a class="active item" data-tab="first"><i class="users icon"></i>Users</a>
        <a class="item" data-tab="second"><i class="world icon"></i>All Tweets</a>
      </div>

      <div class="ui active tab" data-tab="first" style="padding: 10px;">{{> userlistadmin}}</div>
      <div class="ui tab" data-tab="second" style="padding: 10px;">{{> alltweets}}</div>
    </section>
  </div>

  <div class = "four wide align right column padded segment">
      {{> useradd }}
  </div>
</div>

<script type="text/javascript">
  $(document).ready(function(){
    $('.menu .item').tab({history:false});
  });
</script>
