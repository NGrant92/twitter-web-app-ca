{{#each tweets}}
  <div class="ui segment">
    <div class="five wide item">
      <img class="ui two wide avatar image" src="{{user.img}}"/>
      <a class="Header"> {{user.firstName}} {{user.lastName}}</a>
      <p style="word-break:break-all;white-space:normal;">{{message}}</p>
    </div>
  </div>
{{/each}}