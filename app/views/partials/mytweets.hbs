<div class="ui relaxed divided list">
  {{#each myTweets}}
    <div class="item">
      <img class="ui avatar image" src="{{user.img}}"/>
      <div class="content">
        <a class="Header"> {{user.firstName}} {{user.lastName}}</a>
        <p>{{message}}</p>
      </div>
    </div>
  {{/each}}
</div>