<div class="ui card ">
  <div class="image">
    <img src="{{user.img}}">
  </div>
  <div class="content">
    <a class="header">{{user.firstName}} {{user.lastName}}</a>
    <div class="meta">
      <span class="date">Joined in 2017</span>
    </div>
    <table class="ui celled striped table">
      <thead>
      <tr><th colspan="2">
        Details
      </th>
      </tr></thead>
      <tbody>
      <tr>
        <td class="collapsing">
          Email
        </td>
        <td>{{user.email}}</td>
      </tr>
      </tbody>
    </table>
  </div>
</div>